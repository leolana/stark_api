{
   "name": "alpe-api",
   "version": "2.2.0",
   "description": "Api do portal Financeiro (credenciamento e securitização)",
   "main": "src/index.ts",
   "scripts": {
      "start": "nps",
      "build": "npm start build",
      "test": "npm start test",
      "lint": "npm start lint",
      "lint.watch": "npm start lint.watch",
      "precommit": "npm start precommit",
      "commitmsg": "npx commitlint -e $GIT_PARAMS",
      "prettier": "npx prettier --write --",
      "setup": "npm start config && npm start setup.script"
   },
   "dependencies": {
      "@sentry/node": "^5.2.0",
      "aws-param-store": "^2.1.0",
      "aws-sdk": "^2.454.0",
      "body-parser": "^1.19.0",
      "chalk": "^2.4.2",
      "compression": "^1.7.4",
      "connect-flash": "^0.1.1",
      "cookie-parser": "^1.4.4",
      "cors": "^2.8.5",
      "dotenv": "^8.0.0",
      "email-templates": "^5.0.5",
      "express": "^4.16.4",
      "express-flash": "0.0.2",
      "express-jwt": "^5.3.1",
      "express-status-monitor": "^1.2.5",
      "express-validator": "^5.3.1",
      "figlet": "^1.2.1",
      "full-icu": "^1.2.1",
      "helmet": "^3.18.0",
      "http-status-codes": "^1.3.2",
      "inversify": "^5.0.1",
      "jsonfile": "^5.0.0",
      "jsonwebtoken": "^8.5.1",
      "luxon": "^1.13.2",
      "morgan": "^1.9.1",
      "multer": "^1.4.1",
      "nodemailer": "^6.1.1",
      "oracledb": "^3.1.2",
      "pg": "^7.11.0",
      "pg-hstore": "^2.3.2",
      "pug": "^2.0.3",
      "ramda": "^0.26.1",
      "reflect-metadata": "^0.1.13",
      "request": "^2.88.0",
      "request-promise-native": "^1.0.7",
      "sequelize": "^4.42.0",
      "uuid": "^3.3.2",
      "winston": "^3.2.1",
      "winston-transport": "^4.3.0"
   },
   "devDependencies": {
      "@commitlint/cli": "^7.6.1",
      "@commitlint/config-conventional": "^7.6.0",
      "@types/aws-param-store": "^2.1.0",
      "@types/body-parser": "^1.17.0",
      "@types/chalk": "^2.2.0",
      "@types/chance": "^1.0.2",
      "@types/dotenv": "^6.1.1",
      "@types/express": "^4.16.1",
      "@types/express-jwt": "0.0.42",
      "@types/helmet": "0.0.43",
      "@types/jest": "^24.0.12",
      "@types/luxon": "^1.12.0",
      "@types/morgan": "^1.7.35",
      "@types/multer": "^1.3.7",
      "@types/node": "^12.0.1",
      "@types/request-promise-native": "^1.0.16",
      "@types/sequelize": "^4.27.34",
      "@types/uuid": "^3.4.4",
      "chance": "^1.0.18",
      "commitizen": "^3.1.1",
      "commitlint": "^7.6.1",
      "cz-conventional-changelog": "^2.1.0",
      "husky": "^2.2.0",
      "jest": "^24.8.0",
      "lint-staged": "^8.1.6",
      "nodemon": "^1.19.0",
      "nps": "^5.9.5",
      "nps-utils": "^1.7.0",
      "prettier": "^1.17.1",
      "rimraf": "^2.6.3",
      "sequelize-cli": "^5.4.0",
      "sequelize-mock": "^0.10.2",
      "supertest": "^4.0.2",
      "supertest-as-promised": "^4.0.2",
      "ts-jest": "^24.0.2",
      "ts-node": "^8.1.0",
      "tslint": "^5.16.0",
      "tslint-config-airbnb": "^5.11.1",
      "tslint-config-prettier": "^1.18.0",
      "tslint-microsoft-contrib": "^6.1.1",
      "tslint-sonarts": "^1.9.0",
      "typescript": "^3.4.5"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npx lint-staged",
         "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS"
      }
   },
   "config": {
      "commitizen": {
         "path": "npx cz-conventional-changelog"
      }
   },
   "lint-staged": {
      "*.ts": [
         "npx tslint --project tsconfig.json --format stylish --fix",
         "git add"
      ]
   }
}
